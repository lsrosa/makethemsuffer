void loop2(float * v2i1, float * v2i2, float * v2o1, float * v2o2){

  float temp;
  int n21 = 21;
  int n22 = 87;

  for(int i=0; i<=n21; i++){
    for(int j=0; j<=n22; j++){
      temp = 0;
      for(int k=0; k<=n22; k++){
        temp += v2i1[i*n22+k]*v2i2[k+j*n21];
      }
      v2o1[i*n22+j] = temp;
    }
  }
  v2o2 = v2i1;
}
